    "next-optimized-images": "^2.6.2",
    "nextjs-google-analytics": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.0.3",
    "react-firebaseui": "^6.0.0",
